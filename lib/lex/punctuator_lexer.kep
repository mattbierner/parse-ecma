/**
 * @fileOverview Lexers for ECMAScript 5.1 punctuators.
 */
package (
    punctuator)
with
    import 'bennu::parse' {
        label},
    import 'bennu::text' {
        string
        trie}
in {


var punctuators = [
    '{', '}', '(', ')', ',', '[', ']', '.', ';', ',', ':', '?', '&&', '||', '<<', '>>',
    '>>>', '<=', '<', '>=', '>', '===', '!==', '==', '!=', '=', '<<=', '>>=',
    '>>>=', '+=', '-=', '*=', '%=', '++', '--', '!', '~', '&', '|', '^', '+',
    '-', '*', '%', '/', '/='];

/* Lexer
 ******************************************************************************/
punctuator = label('Punctuator Lexer',
    trie punctuators);

}